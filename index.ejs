<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <![endif]-->
    <title>Treedom</title>
    <meta id="share-name" itemprop="name" content="">
    <meta id="share-description" itemprop="description" content="">
    <meta id="share-image" itemprop="image" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="icon" href="http://7xvsf2.com1.z0.glb.clouddn.com/favicon.ico">
    <!--
        html中图片用法
        <img src="<%= require('./src/img/test.jpg') %>"alt="">
    -->
    <% if (process.env.NODE_ENV === 'handover') { %>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <% var cssFile = htmlWebpackPlugin.files.css[css] %>

    <link href="<%= cssFile %>" rel="stylesheet">
    <% } %>

    <% } else { %>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <% var cssFile = htmlWebpackPlugin.files.css[css].substr(htmlWebpackPlugin.files.publicPath.length); %>

    <link href="<%= cssFile %>" rel="stylesheet">
    <% } %>

    <% } %>

    <!-- 授权加代码统计系统 -->
    <script id="__ga" type="text/javascript" src="http://ga.treedom.cn/ga.js?v=0.3.2&pid=9999"></script>

    <!-- rem布局初始化设置start -->
    <script type="text/javascript">
        <%= require('raw-loader!./src/js/app/rem.js') %>
    </script>
    <!-- rem布局初始化设置end -->
</head>
<body>
    <div class="m-main-wrap absf">

        <!-- 开始页 -->
        <div class="m-loading absf">
            <p style="text-align: center;">loading页</p>
        </div>

        <!-- 开始页 -->
        <div class="m-begin absf">
            <p style="text-align: center;">开始页</p>
        </div>

        <!-- 游戏页 -->
        <div class="m-play-wrap absf sizcon fadein800">
            
        </div>

        <!-- 结束页 -->
        <div class="m-end absf fadein800">
            <div class="lastpage absf sizcon">
                <div class="btn-wrap">
                    <div class="btn-toShare btn abcter"></div>
                    <div class="btn-seeAgain btn  abcter"></div>
                </div>
            </div>

            <!-- 分享蒙版 -->
            <div class="share-wrap absf">
                <div class="share-img absf sizcon"></div>
            </div>
        </div>

        <!-- 视频 -->
        <div class="m-video-wrap absf" id="videoWrap"></div>

    </div>
    <script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <script src="//cdn.bootcss.com/zepto/1.0rc1/zepto.min.js"></script>


    <% if (process.env.NODE_ENV === 'handover') { %>
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>

    <% var jsFile = htmlWebpackPlugin.files.chunks[chunk].entry; %>

    <script type="text/javascript" src="<%= jsFile %>"></script>

    <% } %>

    <% } else { %>
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>

    <% var jsFile = htmlWebpackPlugin.files.chunks[chunk].entry.substr(htmlWebpackPlugin.files.publicPath.length); %>

    <script type="text/javascript" src="<%= jsFile %>"></script>

    <% } %>

    <% } %>

    </body>
</html>
